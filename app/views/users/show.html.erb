

<div class = "container">
  <div class="col s12"  style="padding-top:10vh;">
    <div class="card white">
      <div class="card-content center">
        <h1> <%= @user.first_name %>'s Profile</h1>
        <h2> Username: <%= @user.username %> </h2>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col s4">
      <div class="card white">
        <div class="card-content white-text center">
          <i class="large material-icons" style="color:black;">visibility</i>
        </div>
        <div class="card-action center-align  ">
          <a class="waves-effect waves-light btn-large">
          </a>
        </div>
      </div>
    </div>

    <div class="col s4">
      <div class="card white">
        <div class="card-content white-text center">
          <i class="large material-icons" style="color:black;">shopping_cart</i>
        </div>
        <div class="card-action center">
          <a class="waves-effect waves-light btn-large">
          </a>
        </div>
      </div>
    </div>

    <div class="col s4">
      <div class="card white">
        <div class="card-content white-text center">
          <i class="large material-icons" style="color:black;">done_all</i>
        </div>
        <div class="card-action center">
          <a class="waves-effect waves-light btn-large">
          </a>
        </div>
      </div>
    </div>
  </div>
</div>

<%= link_to "Register a Store", new_store_path %>
