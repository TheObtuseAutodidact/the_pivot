
  <div class="checkout container">
    <div class="col s12 m5">
      <div class="card-panel">
        <h3 class="center"> Past Orders: </h3>
          <table class="bordered striped highlight centered responsive-table">
            <thead>
              <tr>
                <th data-field="Status">Status</th>
                <th data-field="CreatedAt">Date Ordered</th>
                <th data-field="TotalPrice">Total Price</th>
                <th data-field="ViewOrder">View Order</th>
              </th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: "partials/orders", collection: current_user.orders, as: :order %>
          </tbody>
          </table>
        </div>
      </div>
    </div>
